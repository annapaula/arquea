{% extends "base.pdf" %}
{% load proto_tags %}
{% block extrastyle %}
td { padding-top:3px; padding-left:2px;}
{% endblock %}
{% block content %}
<h1>Realizado de terceiros</h1>
<h3>{{ entidade }}</h3>
<h3>Processo {{ termo }}</h3>

<table border="1">
{% for p in pagamentos %}
   <tr><td>{{ p.nf }}</td><td>{{ p.sem|moeda }}</td><td>{{ p.com|moeda }}</td></tr>
   {% for r in p.recursos %}
      <tr><td></td><td width="15%"><a href="{{ r.os.primeiro_arquivo.url }}">{{ r.os }}</a> <a href="{{ r.os.contrato.arquivo.url }}">CT</a></td><td>{{ r.quantidade }}</td><td>{{ r.tipo }} - {{ r.referente }}</td><td>{{ r.sem|moeda }}</td><td>{{ r.com|moeda }}</td></tr>
      {% for b in r.beneficiados %}
         <tr style="border-top-color:white;"><td></td><td>{{ b }}</td><td>{{ b.porcentagem }} %</td><td></td><td></td></tr>
      {% endfor %}
   {% endfor %}
{% endfor %}
</table>

{% endblock %}

