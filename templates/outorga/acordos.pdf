{% load proto_tags %}
{% load static %}
<html xmlns="http://www.w3.org/1999/xhtml" lang="pt-br" xml:lang="pt-br" >
<head>
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<style>
@import url('/var/www/django/media/css/pdf.css');
@page {
  size: a4;
  background-image: url('{% get_media_prefix %}{{papelaria.papel_timbrado_retrato_a4}}');
  background-size: contain;
  background-repeat:no-repeat;
  top: {{papelaria.retrato_a4_margem_superior}}cm;
  left: 1.5cm;
  right: 1.5cm;
  bottom: {{papelaria.retrato_a4_margem_inferior}}cm;
  @frame footer {
    -pdf-frame-content: footerContent;
    bottom: 0.5cm;
    margin-right: 0.5cm;
    height: 0.5cm;
    left: -2.3cm;
    } 
} 

</style> 
</head>
<body>
<h1 repeat="1">Distribuição das concessões por acordo nos processos a partir de 2005</h1>

    <table border="0" repeat="1" cellpadding="5">
    <tr><th>Processo</th><th>Acordo</th><th>Modalidade</th><th>Entidade</th><th>Item do termo de outorga</th></tr>
    {% for p in processos %}
           <tr><td>{{ p.processo }}</td></tr>
           {% for a in p.acordos %}
                 <tr><td width="10%"></td><td>{{ a.acordo }}</td></tr>
                 {% for it in a.itens %}
                     <tr><td></td><td width="25%"></td><td width="10%">{{ it.modalidade }}</td><td width="10%">{{ it.entidade }}</td><td>{{ it.descricao }}</td></tr>
                 {% endfor %}
              {% endfor %}
    {% endfor %}
    </table>

  <div id="footerContent">
      pág <pdf:pagenumber example="00" />
  </div> 
</body>
</html>
