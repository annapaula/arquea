{% load proto_tags %}
{% load url from future %}
{% load i18n proto_tags admin_static %}
{% load static %}
{% get_static_prefix as STATIC_PREFIX %}

<style type="text/css">
h1 {
    font-size: 16px;
    color:#134089;
    font-family: Arial, Verdana, Helvetica, sans-serif;
    font-weight: bold;
    postion:relative;
    margin:0;
    display:block;
}
h2 {
    font-family: Arial, Verdana, Helvetica, sans-serif;
    font-size: 15px;
    font-weight: bold;
    color:#333;
}

.rack h2 {
    margin-left:42px;
} 

.tabela tr { height:5px; }

.u {
    background:white repeat-y;
    display: table-cell;
    width:7px;
    max-width:7px;
    min-width:7px;
    vertical-align:top;
    background-image: url('{{ STATIC_PREFIX }}img/1u.png');
}
.bloco-n {
    display: table-cell;
    width:20px;
    background-color:white;
    margin-right:7px;
    vertical-align:top;
    border-bottom:1px solid;
    border-color: #999a9a;
    
}
.bloco-n>div {
    position:relative;
}

.bloco-n .u-n {
    background:#d8d8d8;
    color:#636363;
    font:8px Verdana,Arial;
    border-top:0;
    border-right:1px;
    border-bottom:0;
    border-left:1px; 
    border-style:solid; 
    font-size:9px;
    min-height:19px;
}
.bloco-n .u-n div {
    border-top: 1px solid;
    vertical-align: middle;
    text-align: center;
    line-height: 18px;
}
/**
Coluna de furos do rack
*/
.u-n-left {
    width:7px; 
    display:block; 
    background-color:#eee; 
    position:absolute; 
    left:24px; 
    height: 20px; 
    top:-1px; 
    border-left:#aaa 2px solid; 
    border-right: #888 2px solid;
    border-top:0;
    border-bottom:0;
}
.u-n-right {
    width:7px; 
    display:block; 
    background-color:#eee; 
    position:absolute; 
    left:239px; 
    height: 21px; 
    top:0; 
    border-left:#aaa 2px solid; 
    border-right: #888 2px solid;
}
.u-n-f {
    background:black; 
    position:absolute; 
    width:1px; 
    height:1px;
    padding-top:1px;
    padding-left:1px;
    left:1px;
    border:0; 
}
.f1 {
    top:3px;
}
.f2 {
    top:9px;
}
.f3 {
    top:15px;
}

.rack {
    display:inline-block;
    margin-right:25px;
    margin-bottom:30px;
    font-size:0;
    top:20px;
    position:relative;
}
.outros,
.conflitos {
    display:inline-block;
    width:255px;
}
.equip.profundidade_curta {
    background-color:#ccc;
}
img.profundidade_curta {
    opacity:0.3;
    filter: alpha(opacity=30); /* For IE8 and earlier */
}
.interno {
    position:relative;
    display: table-cell;
    width:199px;
    padding-left:17px;
    font-size:10px;
    top:-3px;
}
.interno img {
    position:absolute;
    z-index: 5;
    width:100%;
    height:100%;
}
.interno div {
    position:absolute;
    z-index: 1;
}
.interno div div div:nth-child(1) {
    position:relative;
    display: table-cell;
    vertical-align:middle;
    text-align: center;
    color:#FFF;
}

.interno div div div:nth-child(2) {
    position: absolute;
    top: 0;
    left: 0;
    vertical-align:middle;
    line-height: 1.5em;
    text-align: center;
    color:#FFF;
    display:none;
}

.interno div div div {
    width:199px;
}

.interno div div div {
    text-shadow: -1px 0 black, 0 1px black, 1px 0 black, 0 -1px black, -2px 0 1px black, 0 2px 1px black, 2px 0 1px black, 0 -2px 1px black;
}
.equip {
    display:inline-block;
    background:#81878b;
    border-bottom:1px solid #fff;
    border-top:1px solid #fff;
    padding-right:1px; 
    padding-left:1px;
    vertical-align:middle; 
    line-height:1.5em;
    width: 199px;
    z-index:1;
}
.equip.pos_T01,
.equip.pos_T01 div,
.equip.pos_01,
.equip.pos_01 div {
    width:97px;
    font-size: 9px;
}
.equip.pos_T02,
.equip.pos_T02 div div,
.equip.pos_02,
.equip.pos_02 div div {
    /*left:16.3em;*/
    right:43px;
    width:97px;
    padding-right: 0;
    margin-right: 0;
    font-size: 9px;
}
.equip.pos_T03,
.equip.pos_T03 div,
.equip.pos_03,
.equip.pos_03 div {
}
.dc {
    margin-top:5px;
}
.outros caption {
    display:block;
    text-align: left;
    font-size: 12px;
    font-weight: bold;
}
.outros table {
    margin-left:0;
    margin-right:0;
    margin-bottom:10px;
    margin-top:10px;
    width:100%;
    border-spacing:0;
    border-collapse: collapse;
}
.outros th,
.outros td {
    border: 1px solid;
    font-family: Arial, Verdana, Helvetica, sans-serif;
    font-size:11px;
}
 .div_pdu {
    display: table-cell;
    width: 30px;
    color: black;
    font-size: 5px;
    padding-left:15px;
}
 .div_pdu a .equip {
    width: 18px;
    margin-left: 4px;
    margin-right: 5px;
    position:absolute;
 }
 .div_pdu a .equip img {
    width: 100%;
    height: 100%;
 }



.page-break {
    page-break-before:always;
    display: block;
    clear: both;
}
/**
    Exibe ou esconde as imagens dos stencils 
*/
 {% if chk_stencil == "0" %}
    .interno img { display:none; }
 {% endif %}
/**
    Exibe ou esconde as legendas dos equipamentos 
*/
 {% if chk_legenda == "0" %}
    .interno div div div:nth-child(1) { display:none; }
 {% endif %}
 
 {% if chk_legenda_desc == "0" %}
    .interno div div div:nth-child(2) { display:none; }
 {% elif chk_legenda_desc == "1" %}
    .interno div div div:nth-child(2) { display:block; }
 {% endif %}
 
 {% if chk_outros == "0" %}
    .outros { display:none; }
 {% endif %}
 {% if chk_avisos == "0" %}
    .conflitos { display:none; }
 {% endif %}
 
   
 
</style>
<h1>Diagrama de Racks Frontal</h1>
    {% for dc in dcs %}
    {% if not forloop.first %}
        <div class="page-break"></div>
    {% endif %}
    <div class="dc">
    <h1>{{ dc.entidade }} - {{ dc.nome|default:'' }}</h1>
    {% for fileira in dc.fileiras %}
        {% if not forloop.first %}
            <p class="page-break"></p>
        {% endif %}
        {% for rack in fileira.racks %}
            {% if not forloop.first %}
                {% if chk_avisos == '1' %}
                    <div class="page-break"></div>
                {% elif chk_outros == '1' and forloop.counter0|divisibleby:"2" %}
                       <div class="page-break"></div>
                {% elif forloop.counter0|divisibleby:"4" %}
                       <div class="page-break"></div>
                {% endif %}
            {% endif %}
            <div class="rack">
                <h2>
                  {{ rack.nome|default:'' }} ({{ rack.vazio }} ocupado)
                </h2>
              <div class="rack_frontal" style="display:table-row">
                    <div class="div_pdu" style="height: {{ rack.altura_pxs }}px;">
                        {% for eq in rack.equipamentos_pdu %}
                            {% if eq.pos_col == 'LE' %}
                               {% if eq.id %}
                                   <a href="{% url "admin:patrimonio_patrimonio_change" eq.id %}" onclick="return showAddAnotherPopup(this);">
                               {% endif %}
                               <div class="equip " rel="tooltip" title="{{ eq.descricao }}" 
                                    style="margin-top:{{ eq.eixoY }}px; height:{{ eq.altura }}px;">
                                    {% if eq.imagem %}<img src="{{ eq.imagem }}" />{% endif %}
                               </div>
                               {% if eq.id %}
                                   </a>
                               {% endif %}
                            {% endif %}
                        {% endfor %}
                    </div>
                    <div id="rack{{ dc.id }}_{{forloop.counter}}" class="bloco-n" style="position:relative; height: {{ rack.altura_pxs }}px;">
                        <script>
                        var content = '';
                        for(i={{rack.altura_pts}};i>0;i--){
                            content += '<div>';
                            content += '<div class="u-n"><div>'+i+'</div></div>';
                            content += '<div class="u-n-left">';
                            content += '<div class="u-n-f  f1"></div>';
                            content += '<div class="u-n-f  f2"></div>';
                            content += '<div class="u-n-f  f3"></div>';
                            content += '</div>';
                            content += '<div class="u-n-right">';
                            content += '<div class="u-n-f  f1"></div>';
                            content += '<div class="u-n-f  f2"></div>';
                            content += '<div class="u-n-f  f3"></div>';
                            content += '</div>';
                            content += '</div>';
                        }
                        document.write(content);
                        </script>
                    </div>
                    
                    <div class="interno">
                    {% for eq in rack.equipamentos %}
                       <div class="{% if eq.pos_col %} pos_{{eq.pos_col}} {% endif %}  equip {% if eq.flag_traseiro and eq.profundidade < 1.0 %}profundidade_curta{% endif %}" 
                           style="margin-top:{{ eq.eixoY }}px; height:{{ eq.altura }}px; {% if not eq.flag_traseiro %} z-index:10;{% endif %}">
                           <div>
                               <div style="height:{{ eq.altura }}px;">
                                {% if eq.pos_col %}
                                    {% if eq.tam >= 15 %}
                                        {{ eq.nome|default:""|truncatechars:80 }}
                                    {% elif eq.tam >= 12 %}
                                        {{ eq.nome|default:""|truncatechars:65 }}
                                    {% elif eq.tam >= 9 %}
                                        {{ eq.nome|default:""|truncatechars:50 }}            
                                    {% elif eq.tam >= 6 %}
                                        {{ eq.nome|default:""|truncatechars:35 }}
                                    {% else %}
                                        {{ eq.nome|default:""|truncatechars:20 }}
                                    {% endif %}
                                {% else %}
                                    {% if eq.tam >= 15 %}
                                        {{ eq.nome|default:""|truncatechars:150 }}
                                    {% elif eq.tam >= 12 %}
                                        {{ eq.nome|default:""|truncatechars:120 }}
                                    {% elif eq.tam >= 9 %}
                                        {{ eq.nome|default:""|truncatechars:90 }}            
                                    {% elif eq.tam >= 6 %}
                                        {{ eq.nome|default:""|truncatechars:60 }}
                                    {% else %}
                                        {{ eq.nome|default:""|truncatechars:30 }}
                                    {% endif %}
                                {% endif %}
                                </div>
                                <div style="height:{{ eq.altura }}px;" style="position:absolute">
                                {% if eq.pos_col %}
                                    {% if eq.tam >= 15 %}
                                        {{ eq.descricao|truncatechars:80 }}
                                    {% elif eq.tam >= 12 %}
                                        {{ eq.descricao|truncatechars:65 }}
                                    {% elif eq.tam >= 9 %}
                                        {{ eq.descricao|truncatechars:50 }}            
                                    {% elif eq.tam >= 6 %}
                                        {{ eq.descricao|truncatechars:35 }}
                                    {% else %}
                                        {{ eq.descricao|truncatechars:20 }}
                                    {% endif %}
                                {% else %}
                                    {% if eq.tam >= 15 %}
                                        {{ eq.descricao|truncatechars:150 }}
                                    {% elif eq.tam >= 12 %}
                                        {{ eq.descricao|truncatechars:120 }}
                                    {% elif eq.tam >= 9 %}
                                        {{ eq.descricao|truncatechars:90 }}            
                                    {% elif eq.tam >= 6 %}
                                        {{ eq.descricao|truncatechars:60 }}
                                    {% else %}
                                        {{ eq.descricao|truncatechars:30 }}
                                    {% endif %}
                                {% endif %}
                                </div>
                            </div>
                            {% if eq.flag_traseiro %}
                                {% if eq.imagem_traseira %}<img src="{{ eq.imagem_traseira }}" {% if eq.profundidade < 1.0 %}class="profundidade_curta"{% endif %} />{% endif %}
                            {% else %}
                                {% if eq.imagem %}<img src="{{ eq.imagem }}" />{% endif %}
                            {% endif %}
                       </div>
                  {% endfor %}
                  </div>
                  <div class="div_pdu" style="height: {{ rack.altura_pxs }}px;">
                    {% for eq in rack.equipamentos_pdu %}
                        {% if eq.pos_col == 'LD' %}
                            {% if eq.id %}
                                <a href="{% url "admin:patrimonio_patrimonio_change" eq.id %}" onclick="return showAddAnotherPopup(this);">
                            {% endif %}
                           <div class="equip " rel="tooltip" title="{{ eq.descricao }}" 
                                style="margin-top:{{ eq.eixoY }}px; height:{{ eq.altura }}px;">
                                {% if eq.imagem %}<img src="{{ eq.imagem }}" />{% endif %}
                           </div>
                            {% if eq.id %}
                                </a>
                            {% endif %}
                        {% endif %}
                    {% endfor %}
                    </div>
                </div>
            </div>
        <!-- end rack -->
        {% endfor %}
    {% endfor %}
{% endfor %}
</div>
{% if chk_traseira %}
<div class="page-break"></div>
<h1>Diagrama de Racks Traseiro</h1>
