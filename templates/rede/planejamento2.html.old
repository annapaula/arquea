{% extends "admin/base_site.html" %}
{% load proto_tags %}
{% block extrahead %}
<script src="/media/js/jquery.js" type="text/javascript"></script>
<script type="text/javascript">
</script>
{% endblock %}
{% block content %}
<h1>Realizado de terceiros</h1>
    <form action="{% url rede.views.planejamento2 1 %}" method="GET">
       <input type="hidden" value="{{ entidade.id }}" name="entidade" />
       <input type="hidden" value="{{ termo.id }}" name="termo" />
       <input type="submit" value="Versão para impressão" />
    </form>

<h3>{{ entidade }}{% if beneficiado %} - {{ beneficiado }} ({{ sem|moeda }}, {{ com|moeda }}){% endif %}</h3>
<h3>Processo {{ termo }}</h3>

<table>
<tr><th>Nota fiscal</th><th>Valor total sem imposto</th><th>Valor total com imposto</th><th>OS e contrato</th><th>Número de vezes que foi pago</th><th>Descrição do item</th><th>Valor unitário sem imposto</th><th>Valor unitário com imposto</th></tr>
{% for p in pagamentos %}
   <tr><td>{{ p.nf }}</td><td>{{ p.sem|moeda }}</td><td>{{ p.com|moeda }}</td></tr>
   {% for r in p.recursos %}
      <tr><td colspan="3"></td><td><a href="{{ r.os.primeiro_arquivo.url }}">{{ r.os }}</a> <a href="{{ r.os.contrato.arquivo.url }}">CT {{ r.os.contrato.numero }}</a></td><td>{{ r.quantidade }}</td><td title="{{ r.tipo }} - {{ r.referente }}">{{ r.tipo|truncatechars:30 }} - {{ r.referente|truncatechars:20 }}</td><td>{{ r.sem|moeda }}</td><td>{{ r.com|moeda }}</td></tr>
      {% for b in r.beneficiados %}
         <tr><td colspan="3"></td><td>{{ b }}</td><td>{{ b.porcentagem }} %</td></tr>
      {% endfor %}
   {% endfor %}
{% endfor %}
</table>
{% endblock %}

